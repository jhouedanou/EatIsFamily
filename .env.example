# Eat Is Family - Environment Variables
# Copy this file to .env and adjust values as needed

# WordPress API Base URL
# Default: https://bigfive.dev/eatisfamily/index.php/wp-json/eatisfamily/v1
NUXT_PUBLIC_API_BASE_URL=https://bigfive.dev/eatisfamily/index.php/wp-json/eatisfamily/v1

# Use local JSON files instead of WordPress API
# Set to 'true' to use local fallback files in /public/api/
# Default: false
NUXT_PUBLIC_USE_LOCAL_FALLBACK=false
